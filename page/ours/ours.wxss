page{
  background: url("http://cdn.zjwoo.com/film/background.png") no-repeat;
  background-size: cover;
  padding-bottom: 50rpx
}

.container {
  padding: 30rpx 20rpx;
  text-align: center;
  /* background-image: url("https://images.unsplash.com/photo-1551334787-21e6bd3ab135?w=640"); */
  margin-bottom: 180rpx;
}



.aboutUs-logo image{
  width: 290rpx;
  height: 60rpx;
}

.aboutUs-des{

}

.aboutUs-des_header, .aboutUs-contact_header{
  font-size: 32rpx;
  font-weight: 600;
  color: #fff;
  margin: 20rpx 0;
}
.aboutUs-des_content {
  text-align: left!important;
  text-indent: 60rpx;
  font-size:30rpx;
  color: #fff;
  min-height: 100rpx;
}

.aboutUs-contact_item{
  width: 100%;
  border-bottom: 1px dotted #fff;
  overflow: hidden;
  margin-bottom: 46rpx;
}
.aboutUs-contact_item image{
  width: 42rpx;
  height: 36rpx;
  float: left;
}
.aboutUs-contact_item text{
  font-size: 30rpx;
  color: #fff;
  float: right;
}

.aboutUs-location{
  width: 100%;
  margin-top: 80rpx;
}
.aboutUs-location_item{
  width: calc((100% - 40rpx) / 2 );
  display: inline-block;
  margin-right: 20rpx
}
.aboutUs-location_item image{
  width: 100%;
  height: 300rpx;
}